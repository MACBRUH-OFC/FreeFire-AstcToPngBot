{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "30mb",
        "includeFiles": ["astcenc"],
        "buildCommand": "chmod +x astcenc && pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py",
      "methods": ["GET", "POST"]
    }
  ]
}
